{
  "version": "0.2.0",
  "configurations": [
    {
        "name": "Run on Kubernetes",
        "type": "cloudcode.kubernetes",
        "request": "launch",
        "skaffoldConfig": "${workspaceFolder}/skaffold.yaml",
        "watch": true,
        "cleanUp": true,
        "portForward": true,
        "internalConsoleOptions": "neverOpen",
        "imageRegistry": "gcr.io/cohesive-slate-308002"
    },
    {
        "type": "go",
        "request": "launch",
        "name": "Launch (local)",
        "mode": "auto",
        "program": "${workspaceFolder}/cmd/hello-world"
    }
]
}
